{
  "name": "@teitei-tk/anime.annict.js",
  "version": "1.3.1",
  "main": "lib/index.js",
  "repository": "ssh://git@github.com/teitei-tk/anime.annict.js.git",
  "author": "teitei-tk <teitei.tk@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "yarn lint && tsc",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "commit": "yarn git-cz",
    "publish:prod": "yarn build && yarn publish"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "graphql": "^15.0.0",
    "humps": "^2.0.1"
  },
  "devDependencies": {
    "@types/humps": "^1.1.3",
    "@types/jest": "^25.1.5",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.6",
    "lint-staged": "^10.1.1",
    "nock": "^12.0.3",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib"
    ],
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
